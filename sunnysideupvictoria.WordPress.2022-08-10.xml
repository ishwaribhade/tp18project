		<dc:creator><![CDATA[tp18]]></dc:creator>
		<guid isPermaLink="false">https://sunnysideupvictoria.lovestoblog.com/?page_id=86</guid>
		<description></description>
		<content:encoded><![CDATA[<!-- wp:html -->
<style>
/* background colour for the page*/
body {
    background-color: #FFFFFF;
}
.div-1 {
        background: white;
        padding: 1em;
    }
</style>
<!DOCTYPE html>
<body>
<script>
// In this function I would hope to retrieve the UV level local to the user by looking up the dataset or API. For now, I have set this to an arbitrary number, but the output of displayMessage() will change depending on this value.
function getUV() {
  return 3;
}
// Want to categorise the severity level of UV into Low (Green colour; UV in range 1-2), Medium (Yellow colour; UV in range 3-5), High (Orange colour; UV in range 6-7), Very high (Red colour; UV in range 8-10), Extreme (Purple colour; UV in range 11+)
function chooseColour(uv) {
    if (uv <= 2) {
        return "lime";
    }
    else if (uv <= 5) {
        return "yellow";
    }
    else if (uv <= 7) {
        return "orange";
    }
    else if (uv <= 10) {
        return "red";
    }
    else {
        return "purple"; // a version of purple
    }
}
// Based on the categorisation of severity of UV level, return the appropriate message for the user and display this to the webpage.
function displayMessage() {
    const uv = getUV();
    const colour = chooseColour(uv);
    if (colour == "lime") {
        var uvRating = "LOW";
        var message = "Low risk of UV damage. Only wear sunglasses to look cool.";
	}
    else if (colour == "yellow") {
        var uvRating = "MEDIUM";
        var message = "Stay in shade at noon. Wear a hat and sunscreen to minimize sun damage.";
	}
    else if (colour == "orange") {
        var uvRating = "HIGH";
        var message = "There is a high UV risk - Make sure to wear a hat and sunscreen if you plan on going outdoors today.";
    }
	else if (colour == "red") {
        var uvRating = "VERY HIGH";
        var message = "There is a severe UV risk today – Try to stay indoors! If you go out, make sure to wear a hat and sunscreen. It is also recommended to go out only during early morning or late evening hours to reduce your UV risk";
    }
	else {
        var uvRating = "EXTREME";
        var message = "Don't go outside.";
	}
    document.getElementById("feature").style.background = colour; 
    document.getElementById("feature").style.border = "3px solid #000000";
    document.getElementById("uv").innerHTML = "Your UV level is: " + uv;
    document.getElementById("uvRating").innerHTML = "UV Rating: " + uvRating;
    document.getElementById("message").innerHTML = "Suggested actions: " + message;
}
</script>
